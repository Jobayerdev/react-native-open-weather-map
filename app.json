{
	"expo": {
		"name": "NexWeatherMap",
		"slug": "nexweathermap",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/logo.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"description": "Weather Map API",
		"githubUrl": "https://github.com/Jobayerdev/react-native-open-weather-map"
	}
}
